<!DOCTYPE html>
<html>

<% include ../partials/head.ejs %>

  <body>
    <header>
      <nav>
        <div class="nav-wrapper">
          <a href="#" class="brand-logo">That Dang Family</a>
          <div class="row">
            <div class="col s6 offset-s6">
              <div class="input-field col s5">
                <label for="login-email">Email</label>
                <input type="text" name="email" id="login-email" class="validate">
              </div>
              <div class="input-field col s5">
                <input type="text" name="password" id="login-password">
                <label for="login-password">Password</label>
              </div>
              <div class="input-field col s2">
                <a id="login" class="waves-effect waves-light btn">LOG IN</a>
              </div>
            </div>
          </div>
      </nav>
    </header>
    <main>
      <div class="container">

        <div class="row">
          <div class="col s12 m6">
            <h1>Welcome!</h1>
            <div class="card white darken-1 valign-wrapper hoverable">
              <div class="card-content center-align green-text">
                <span class="card-title center-align">Sign Up</span>
                <p class="center-align">Start building your family tree today.</p>
                <a class="waves-effect waves-light btn-large center-align">Register</a>

              </div>
            </div>
          </div>
        </div>
      </div>
    </main>

    <% include ../partials/footer.ejs %>
    <script src="/js/index.js"></script>
  </body>

</html>
