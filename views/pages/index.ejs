        <div class="row">
          <div class="col s4 offset-s8">
            <form>
              <div class="input-field col s6">
                <label for="login-email">Email</label>
                <input type="text" name="email" id="login-email" class="validate">
              </div>
              <div class="input-field col s6">
                <input type="text" name="password" id="login-password">
                <label for="login-password">Password</label>
              </div>
            </form>
          </div>
        </div>
    </nav>
  </header>
  <main>

    <div class="row">
            <div class="col s12 m6">
              <h1>Welcome!</h1>
              <div class="card white darken-1 valign-wrapper hoverable">
                <div class="card-content center-align green-text">
                  <span class="card-title center-align">Sign Up</span>
                  <p class="center-align">Start building your family tree today.</p>
                  <a class="waves-effect waves-light btn-large center-align">Register</a>

                </div>
              </div>
            </div>
          </div>

  </main>
  <footer class="page-footer">
    <% include ../partials/footer.ejs %>
  </footer>
  <script type="text/javascript" src="js/jquery.min.js"></script>
